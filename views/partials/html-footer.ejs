    <script type="text/javascript" src="/components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="/components/jquery-ui/jquery-ui.min.js"></script>
    <script type="text/javascript" src="/components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/components/smoothScroll/smoothscroll.js"></script>
    <script type="text/javascript" src="/components/gridalicious/jquery.grid-a-licious.min.js"></script>
    <script type="text/javascript" src="/minified/js/grid.js"></script>
    <script type="text/javascript" id="cookiebanner" src="/components/cookiebanner/dist/cookiebanner.min.js"></script>
    <script type="text/javascript" src="/components/remarkable-bootstrap-notify/dist/bootstrap-notify.min.js"></script>
    
      
  <% if(error && error.length > 0) { %>
    <script>
      $.notify({
      	// options
      	icon: 'fa fa-exclamation-circle',
      	allow_dismiss: true,
      	message: '<%= error %>'
      },{
      	// settings
      	type: 'danger',
      	placement: {
      		from: "top",
      		align: "right"
      	},
      	offset: 20,
      	spacing: 10,
      	z_index: 1031,
      	mouse_over: 'pause',
      	delay: 5000,
      	animate: {
      		enter: 'animated fadeInDown',
      		exit: 'animated fadeOutUp'
      	}
      }).delay( 1300 );
    </script>
  <% } else if(success && success.length > 0) { %>
    <script>
      $.notify({
      	// options
      	icon: 'fa fa-check',
      	allow_dismiss:true,
      	message: '<%= success %>' 
      },{
      	// settings
      	type: 'success',
      	placement: {
      		from: "top",
      		align: "right"
      	},
      	offset: 20,
      	spacing: 10,
      	z_index: 1031,
      	mouse_over: 'pause',
      	delay: 5000,
      	animate: {
      		enter: 'animated fadeInDown',
      		exit: 'animated fadeOutUp'
      	}
      }).delay( 1300 ); 
    </script>
  <% } %>  
